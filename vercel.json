{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist/spa" } }
  ],
  "env": {
    "NODE_VERSION": "18"
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
